<?xml version="1.0" encoding="UTF-8"?>
<beans
        xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
        default-autowire="constructor"
        >
  <!-- declare all beans you like Spring Dependency Injection to create -->
  <!-- see http://static.springsource.org/spring/docs/3.0.x/spring-framework-reference/html/beans.html -->

  <!-- sample: 
  <bean class="bean class fully qualified name"/>
  -->

  <bean class="jetbrains.teamcity.depsOrder.server.OrderManager"/>
  <bean class="jetbrains.teamcity.depsOrder.server.SettingsManager"/>

  <bean class="jetbrains.teamcity.depsOrder.server.ui.OrderingFeature"/>
  <bean class="jetbrains.teamcity.depsOrder.server.ui.FeatureController"/>
  <bean class="jetbrains.teamcity.depsOrder.server.ui.FeatureControllerRegister"/>
  <bean class="jetbrains.teamcity.depsOrder.server.ui.Paths"/>

</beans>
